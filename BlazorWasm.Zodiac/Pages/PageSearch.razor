@using BlazorWasm.Zodiac.Data
@using BlazorWasm.Zodiac.Models
@if (!_isSearch)
{
    <section class="as_appointment_wrapper as_padderBottom40 as_padderTop100">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h1 class="as_heading as_heading_center">Search Zodiac</h1>
                    <span>
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="240" height="15" viewBox="0 0 240 15">
                            <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="240" height="15" xlink:href="data:img/png;base64," />
                        </svg>
                    </span>
                </div>
                <div class="col-lg-10 offset-lg-1 col-md-10 offset-md-1 col-sm-12">
                    <form class="as_appointment_form">
                        <div class="row">
                            <div class="col-lg-3 col-md-3"></div>
                            <div class="col-lg-6 col-md-6 col-sm-12">
                                <div class="form-group">
                                    @* <input type="date" placeholder="Choose Date" class="form-control as_datepicker"> *@
                                    <input type="text" placeholder="Choose Date" @bind="_date" id="txtChooseDate" class="form-control as_datepicker">
                                    @* <span><img src="assets/images/svg/calendar.svg" alt=""></span> *@
                                </div>
                                <div class="col-lg-12" style="text-align: center;">
                                    <button class="as_btn" type="button" @onclick="SearchDate">Search</button>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3"></div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
}
else
{
    <section class="as_service_wrapper as_padderTop80 as_padderBottom80">
        <div class="container">
            <div class="col-lg-12" style="text-align: center;">
                <button class="as_btn" type="button" @onclick="Back">Back</button>
            </div>
            <div class="row as_verticle_center">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="as_sign_box text-center">
                        <a href="javascript:void(0);">
                            <span class="as_sign">
                                <img src="assets/images/svg/horoscope/h@(_index + 1).svg" alt="">
                            </span>
                            <div>
                                <h5>@_zodiacSign.Name</h5>
                                <p>@_zodiacSign.Dates</p>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
            <div class="text-center">
                <div class="row">
                    <div class="col-lg-3 col-md-4 col-sm-4">
                        <div class="as_service_box text-center" style="height: 95%;">
                            <h3> ဒြပ် / ELEMENT </h3>
                            <h3> @_zodiacSign.Element </h3>
                            <img src="images/elements/@(_zodiacSign.Element).png" alt="">
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6">
                        <h1 class="as_heading as_heading_center" style="margin-top: 20px">Personality Traits</h1>
                        <span>
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="240" height="15" viewBox="0 0 240 15">
                                <image id="Vector_Smart_Object" data-name="Vector Smart Object" width="240" height="15" xlink:href="data:img/png;base64,"></image>
                            </svg>
                        </span>
                        <p class="as_font14 as_padderTop20 as_padderBottom20"></p>
                        <div id="chart"></div>
                    </div>
                    <div class="col-lg-3 col-md-4 col-sm-4">
                        <div class="as_service_box text-center" style="height: 95%;">
                            <h3> မြန်မာလ </h3>
                            <h5> - </h5>
                            <h3> @_zodiacSign.MyanmarMonth </h3>
                            <h3> Sign </h3>
                            <span class="as_sign">
                                <img src="@(_zodiacSign.ZodiacSignImageUrl)" alt="" style="width:85px; height: 85px">
                            </span>
                            <img src="@(_zodiacSign.ZodiacSign2ImageUrl)" alt="">
                        </div>
                    </div>
                </div>
            </div>
            <div class="align-content-center">
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Life Purpose</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.LifePurpose</h5>
                </div>
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Loyal</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.Loyal</h5>
                </div>
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Angry</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.Angry</h5>
                </div>
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Character</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.Character</h5>
                </div>
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Pretty Features</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.PrettyFeatures</h5>
                </div>
                <div class="as_customer_box text-center mb-2">
                    <h1 class="as_heading as_heading_center">Representative Flower</h1>
                    <h5 style="color:black; line-height: 1.5;">@_zodiacSign.RepresentativeFlower</h5><br>
                    <img src="images/representative-flower/@(_index + 1).jpg" alt="" style="width:250px; height:250px">
                </div>
            </div>
        </div>
    </section>
}
